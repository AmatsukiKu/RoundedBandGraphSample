<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="30dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <com.example.gonnokami.roundedbandgraphsample.RoundedBandGraph4
            android:id="@+id/graphFull"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginBottom="20dp" />

        <com.example.gonnokami.roundedbandgraphsample.RoundedBandGraph4
            android:id="@+id/graphAlmost"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginBottom="20dp" />

        <com.example.gonnokami.roundedbandgraphsample.RoundedBandGraph4
            android:id="@+id/graphMost"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginBottom="20dp" />

        <com.example.gonnokami.roundedbandgraphsample.RoundedBandGraph4
            android:id="@+id/graphHalf"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginBottom="20dp" />

        <com.example.gonnokami.roundedbandgraphsample.RoundedBandGraph4
            android:id="@+id/graphSome"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginBottom="20dp" />

        <com.example.gonnokami.roundedbandgraphsample.RoundedBandGraph4
            android:id="@+id/graphLittle"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginBottom="20dp" />

        <com.example.gonnokami.roundedbandgraphsample.RoundedBandGraph4
            android:id="@+id/graphEmpty"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:layout_marginBottom="20dp" />

    </LinearLayout>

</ScrollView>
